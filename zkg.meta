description = Analyzes protocol buffer files and generates Zeek events.

[template]
source = https://github.com/zeek/package-template
version = v2.0.0
zkg_version = 2.13.0
features = plugin

[template_vars]
name = ProtobufAnalyzer
namespace = Demo

